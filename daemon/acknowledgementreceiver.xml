<service>
	<id>acknowledgementreceiver.exe</id>
	<name>AcknowledgementReceiver</name>
	<description>Receives signed handbook PDFs and saves to OneDrive</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\acknowledgement-receiver\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\acknowledgement-receiver\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>AcknowledgementReceiver wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="OUTPUT_DIR" value="C:\Users\tgaut\OneDrive - Advantage Solutions\Signed Acknowledgements"/>
	<serviceaccount>
		<domain>OFFICEPC</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\acknowledgement-receiver</workingdirectory>
</service>